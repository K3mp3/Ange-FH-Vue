{
  "name": "vue-sign-in-form",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "pre-commit": "lint-staged",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" && echo 'Lint checked.'",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --fix && echo 'Lint fix complete.'",
    "lint:prettier": "prettier --config .prettierrc.json 'src/*.{js,jsx,ts,tsx}' --write"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@splidejs/vue-splide": "^0.6.12",
    "gsap": "^3.12.1",
    "pinia": "^2.1.4",
    "sass": "^1.62.1",
    "swiper": "^10.0.3",
    "vue": "^3.2.47",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^4.1.6",
    "vue-touch": "^2.0.0-beta.4",
    "vue3-carousel": "^0.3.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.4",
    "vue-tsc": "^1.6.4"
  }
}
